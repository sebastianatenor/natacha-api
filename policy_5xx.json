      "duration": "300s",
      "filter": "metric.type=\"run.googleapis.com/request_count\" resource.type=\"cloud_run_revision\" resource.label.\"service_name\"=\"natacha-api\" metric.label.\"response_code_class\"=\"5xx\"",
